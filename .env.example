# VaultNotary Environment Variables Template
# Copy this file to .env and fill in your actual values

# Application Environment
ASPNETCORE_ENVIRONMENT=Test

# Database Configuration
DATABASE_CONNECTION_STRING=Host=your-db-host;Port=5432;Database=your-db-name;Username=your-username;Password=your-password

# AWS Configuration
AWS_REGION=your-aws-region
AWS_USE_IAM_ROLE=false
AWS_ACCESS_KEY=your-aws-access-key
AWS_SECRET_KEY=your-aws-secret-key

# AWS Resources
AWS_S3_BUCKET_NAME=your-s3-bucket-name
AWS_S3_FILE_PREFIX=files/
AWS_S3_PRESIGNED_URL_HOURS=24
AWS_KMS_SYMMETRIC_KEY_ID=your-kms-key-id

# Message Queue Configuration
RABBITMQ_CONNECTION_STRING=amqp://admin:password@rabbitmq:5672

# RabbitMQ
RABBITMQ_DEFAULT_USER=admin
RABBITMQ_DEFAULT_PASS=password

# Environment
NODE_ENV=development

# RATE LIMITING
RATE_LIMIT_WINDOW=15
RATE_LIMIT_MAX_REQUESTS=100